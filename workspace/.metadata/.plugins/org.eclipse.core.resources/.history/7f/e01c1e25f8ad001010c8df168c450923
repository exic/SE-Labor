package src.domain.figure;

import java.awt.Color;

public class Wissensstandsanzeiger implements IWissensstandsanzeiger{

	public Color getFarbe() {
		return farbe;
	}
	public int getLevel() {
		return level;
	}
	public void setLevel(int level) {
		this.level = level;
	}
	private Color farbe;
	private int level;
}
